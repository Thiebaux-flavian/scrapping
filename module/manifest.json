{
  "manifest_version": 2,
  "name": "scrapping",
  "description": "Allez, appuye",
  "version": "1.0",
  "permissions": [
                "http://*/*",
            	"https://*/*"
  ],
  "content_scripts": [
      {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["jquery.js","main.js"],
          "css" : ["style.css"]
      }
  ],
  "browser_action": {
    "default_icon": "ok.png"
  }
}